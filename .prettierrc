{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "jsxSingleQuote": false,
  "jsxBracketSameLine": true,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 5
      }
    }
  ],
  "printWidth": 140,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false
}
